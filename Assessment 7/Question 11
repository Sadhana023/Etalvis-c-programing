#include <stdio.h>
int count_total_digits(int num){
    int count=0;
    if(num==0) return 1;
    if(num<0) num=-num;
    while(num>0){
        num/=10;
        count++;
    }
    return count;
}
int main(){
    int num;
    scanf("%d",&num);
    printf("%d",count_total_digits(num));
    return 0;
}
