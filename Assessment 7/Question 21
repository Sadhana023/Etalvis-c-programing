#include <stdio.h>
int disp_total_odd_digits(int num)
{
    if(num<0) num=-num;
    int count=0;
    while(num>0)
    {
        if((num%10)%2!=0) 
        count++;
        num/=10;
    }
    return count;
}
int main()
{
    int num;
    scanf("%d",&num);
    printf("%d",disp_total_odd_digits(num));
    return 0;
}
