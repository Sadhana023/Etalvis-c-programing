#include <stdio.h>

void disp_single_digit_square(int n);

int main()
{
    int n;
    printf("Enter number: ");
    scanf("%d", &n);

    disp_single_digit_square(n);
    return 0;
}

void disp_single_digit_square(int n)
{
    int d1, d2, twoDigit, count = 0;

    while (n >= 10)
    {
        d1 = n % 10;
        d2 = (n / 10) % 10;

        twoDigit = d2 * 10 + d1;

        if (twoDigit == 16 || twoDigit == 25 || twoDigit == 36 ||
            twoDigit == 49 || twoDigit == 64 || twoDigit == 81)
        {
            count++;
        }

        n = n / 10;
    }

    printf("Total two digit perfect squares: %d", count);
}
