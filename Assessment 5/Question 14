#include <stdio.h>
int main()
{
    int n, temp, first, last, pow10 = 1;
    scanf("%d", &n);
    temp = n;
    last = n % 10;
    for (; temp >= 10; temp = temp / 10)
    {
        pow10 = pow10 * 10;
    }
    first = temp;
    int middle = n % pow10;
    middle = middle / 10;
    int result = last * pow10 + middle * 10 + first;
    printf("%d", result);
    return 0;
}
