#include <stdio.h>
int main()
{
    int num,count=0,temp,digit,sum,i;
    for(i=1;i<100000;i++)
    {
        temp=i;
        sum=0;
        while(temp!=0)
        {
            digit=temp%10;
            sum+=digit;
            temp/=10;
        }
        if(sum==14)count++;
    }
    printf("%d",count);
    return 0;
}
